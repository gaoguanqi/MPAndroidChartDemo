<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.maple.smaple.mpandroidchartdemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".linechart.LineChartActivity" />
        <activity android:name=".barchart.BarChartActivity" />
        <activity android:name=".combinedchart.CombinedChartActivity" />
        <activity android:name=".piechart.PieChartActivity" />
        <activity android:name=".scatterchart.ScatterChartActivity" />
        <activity android:name=".candlestickchart.CandleStickChartActivity" />
        <activity android:name=".bubblechart.BubbleChartActivity" />
        <activity android:name=".radarchart.RadarChartActivity" />
        <activity android:name=".login.LoginActivity" />
        <activity android:name=".kchart.KChartActivity"></activity>
    </application>

</manifest>